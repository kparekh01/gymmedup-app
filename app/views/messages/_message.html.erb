
<div class="message">
  <a href="" class="message_profile-pic" style="background-image:url(<%= message.user.photo.url %>)"></a>
  <a href="" class="message_username"><%= message.user.user_name %></a>
  <span class="message_timestamp">
    <%= message.created_at %>
  </span>
  <span class="message_star"></span>
  <span class="message_content">
    <%= message.content %>
  </span>
</div>
